					<img src="/_ui/img/locations/princi-locations-carousel-2.jpg" class="" alt="">
					<img src="/_ui/img/locations/princi-locations-carousel-3.jpg" class="" alt="">
					<img src="/_ui/img/locations/princi-locations-carousel-4.jpg" class="" alt="">
				</div>
			</div>

			<section class="city wow fadeIn">
				<h2 class="large-underline"><span>Milan</span></h2>
				<div class="location-group">
					<div class="location quarter" itemscope itemtype="http://schema.org/Place">
						<span class="offscreen" itemprop="containedInPlace"></span>
						<address itemprop="address">Piazza XXV Aprile, 5</address>
						<span itemprop="telephone" class="tel">
							Tel <a href="tel:+390229060832">+39 (0)2 29060832</a>
						</span>

						<p>Monday&mdash;Wednesday<br /> 7:00 AM&mdash;9:00 PM</p>
						<p>Thursday&mdash;Sunday<br /> open 24 hours</p>
					</div>

					<div class="location quarter" itemscope itemtype="http://schema.org/Place">
						<span class="offscreen" itemprop="containedInPlace"></span>
