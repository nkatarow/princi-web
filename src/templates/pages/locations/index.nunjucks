<!-- index.nunjucks -->
{% extends "partials/layout.nunjucks" %}

{% block content %}
	<div id="content">
		<script type="text/javascript">
			uo_data.pageInfo = { // All Pages
				"pageTemplate": "Locations", // Offerings About Locations
			    "pageUrl": "http://www.princi.com/locations/", // Full URL
			}
		</script>

		<div class="parallax">
			<div class="food-hero wow fadeIn">
				<div class="">
					<h1 class="h1">Locations</h1>
					<span class="line"></span>
					<p class="intro">Eat, drink, and live Italian.</p>
